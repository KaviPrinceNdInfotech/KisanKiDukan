@model KisanKiDukan.Models.ViewModels.PayneerForm
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Paynear Checkout</title>
    <link href="/css/bootstrap.css" rel="stylesheet" />
</head>
<body>
    <div>
        @using (Html.BeginForm("PaynearCheckoutPost", "Checkout", FormMethod.Post, new { id = "frmPaynear" }))
        {
            <div class="fullwidth" style="visibility:hidden">
                <div class="container bottomPddng commonBorder">
                    <div class="row rowsTitlebgForm2"></div>
                    <div class="row rowsTitlebgForm2 activeRowBg"><strong>Transaction Details</strong></div>
                    <div class="row rowsTitlebgForm2">
                        <div class="col-xs-12 col-sm-4 col-md-3"> * Reference No </div>
                        <div class="col-xs-12 col-sm-8 col-md-5">
                            @Html.TextBoxFor(a => a.referenceNo, new { @class = "form-control" })
                        </div>
                    </div>
                    <!--  outletId should be '0' -->
                    <div class="row rowsTitlebgForm2">
                        <div class="col-xs-12 col-sm-4 col-md-3"> * Outlet ID </div>
                        <div class="col-xs-12 col-sm-8 col-md-5">
                            @Html.TextBoxFor(a => a.outletId, new { @class = "form-control" })
                        </div>
                    </div>
                    <!-- apiVersion should be '2.0.0'-->
                    <div class="row rowsTitlebgForm2">
                        <div class="col-xs-12 col-sm-4 col-md-3"> * API Version </div>
                        <div class="col-xs-12 col-sm-8 col-md-5">
                            @Html.TextBoxFor(a => a.apiVersion, new { @class = "form-control" })
                        </div>
                    </div>
                    <!-- currencyCode should be 'INR'-->
                    <div class="row rowsTitlebgForm2">
                        <div class="col-xs-12 col-sm-4 col-md-3"> * Currency Code </div>
                        <div class="col-xs-12 col-sm-8 col-md-5">
                            @Html.TextBoxFor(a => a.currencyCode, new { @class = "form-control" })
                        </div>
                    </div>
                    <!-- locale should be 'EN'-->
                    <div class="row rowsTitlebgForm2">
                        <div class="col-xs-12 col-sm-4 col-md-3"> * Locale </div>
                        <div class="col-xs-12 col-sm-8 col-md-5">
                            @Html.TextBoxFor(a => a.locale, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="row rowsTitlebgForm2">
                        <div class="col-xs-12 col-sm-4 col-md-3"> * Description </div>
                        <div class="col-xs-12 col-sm-8 col-md-5">
                            @Html.TextAreaFor(a => a.description, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="row rowsTitlebgForm2">
                        <div class="col-xs-12 col-sm-4 col-md-3"> * Amount </div>
                        <div class="col-xs-12 col-sm-8 col-md-5">
                            @Html.TextBoxFor(a => a.amount, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="row rowsTitlebgForm2">
                        <div class="col-xs-12 col-sm-4 col-md-3">
                            * Response
                        </div>
                        <div class="col-xs-12 col-sm-8 col-md-5">
                            @Html.TextBoxFor(a => a.responseURL, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="row rowsTitlebgForm2">
                        <div class="col-xs-12 col-sm-4 col-md-3"> * Channel </div>
                        <div class="col-xs-12 col-sm-8 col-md-5">
                            @Html.TextBoxFor(a => a.channel, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-6 bdrRght">
                            <div class="row rowsTitlebgForm activeRowBg bdrRght2"><asp:Label ID="address1_type" runat="server"><strong> Billing Address </strong></asp:Label></div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> * Name </div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.billingContactName, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> * Address</div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextAreaFor(a => a.billingAddress, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> * City</div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.billingCity, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> * State</div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.billingState, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> * Postal Code</div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.billingPostalCode, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> * Country</div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.billingCountry, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> * Email</div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.billingEmail, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> * Telephone</div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.billingPhone, new { @class = "form-control" })
                                </div>
                            </div>
                        </div> <!-- billing info end -->
                        <div class="col-xs-12 col-sm-12 col-md-6">
                            <div class="row rowsTitlebgForm activeRowBg"><asp:Label ID="address2_type" runat="server"><strong>Shipping Address</strong></asp:Label></div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> Name </div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.shippingContactName, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> Address </div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.shippingAddress, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> City </div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.shippingCity, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> State </div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.shippingState, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> Postal Code </div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.shippingPostalCode, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> Country </div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.shippingCountry, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> Email </div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.shippingEmail, new { @class = "form-control" })
                                </div>
                            </div>
                            <div class="row rowsTitlebgForm">
                                <div class="col-xs-12 col-sm-4 col-md-3"> Telephone </div>
                                <div class="col-xs-12 col-sm-8 col-md-9">
                                    @Html.TextBoxFor(a => a.shippingPhone, new { @class = "form-control" })
                                </div>
                            </div>
                        </div> <!-- billing info end -->
                    </div> <!-- row end -->
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 rowsTitlebgForm text-center topMargn2">
                            <button type="submit" class="btn btn-primary">Submit</button>
                            <span id="form_validator" style="color:red"></span>
                            <div class="col-xs-12 col-sm-12 col-md-12 topMargn2"> * denotes required field </div>
                        </div>
                    </div>
                </div>
            </div>
        } 
    </div>
</body>
</html>
<script src="/js/jquery.min.js"></script>
<script>
    $("#frmPaynear").submit();
</script>