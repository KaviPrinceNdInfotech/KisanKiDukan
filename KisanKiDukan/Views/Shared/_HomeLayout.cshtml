    <script>
        $(document).on('keydown.autocomplete focus.autocomplete', '#pos_query_top', function () {
        $(this).autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: '/Home/GetProduct',
                    data: { term: request.term },
                    type: 'GET',
                    success: function (data) {

                        if (!data.length) {

                            var result = [
                             {
                                 label: 'No data found',
                                 value: response.term
                             }
                            ];
                            response(result);
                        }
                        else {
                            response($.map(data, function (item) {
                                return {
                                    label: item.ProductName + '(' + item.ProductName + ')',
                                    value: item.ProductName,
                                    id: item.Id,
                                };
                            }));
                        }
                    }
                });
            },
            select: function (event, ui) {
                window.location.href = "/Home/ProductDetail?productId="+ui.item.id;
            }
        })
    });
    </script>

