<div class="freshio-breadcrumb">
    <div class="col-full">
        <h1 class="breadcrumb-heading">Business Details</h1>
        <nav class="woocommerce-breadcrumb"><a href="/Home/Index">Home</a><span class="breadcrumb-separator"> / </span>Business Details</nav>
    </div>
</div>
<div id="content" class="site-content" tabindex="-1">
    <div class="col-full">
        <div class="woocommerce"></div>
        <div id="primary">
            <main id="main" class="site-main">
                <article id="post-88" class="post-88 page type-page status-publish hentry">
                    <div class="entry-content">
                        <div data-elementor-type="wp-page" data-elementor-id="88" class="elementor elementor-88" data-elementor-settings="[]">
                            <div class="elementor-inner">
                                <div class="elementor-section-wrap">
                                    <div class="elementor-element elementor-element-39c81f0 elementor-section-stretched elementor-section-boxed elementor-section-height-default elementor-section-height-default elementor-section elementor-top-section"
                                         data-id="39c81f0"
                                         data-element_type="section"
                                         data-settings='{"stretch_section":"section-stretched"}'>
                                        <div class="elementor-container elementor-column-gap-no">
                                            <div class="elementor-row">
                                                <div class="elementor-element elementor-element-99e7d44 elementor-column elementor-col-50 elementor-top-column" data-id="99e7d44" data-element_type="column">
                                                    <div class="elementor-column-wrap elementor-element-populated">
                                                        <div class="elementor-widget-wrap">
                                                            <div class="elementor-element elementor-element-14eefa1 elementor-widget elementor-widget-heading" data-id="14eefa1" data-element_type="widget" data-widget_type="heading.default">
                                                                <div class="elementor-widget-container"><h2 class="elementor-heading-title elementor-size-default">Business Details</h2></div>
                                                            </div>
                                                            <div class="elementor-element elementor-element-3477432 elementor-button-align-stretch elementor-widget elementor-widget-form"
                                                                 data-id="3477432"
                                                                 data-element_type="widget"
                                                                 data-settings='{"button_width":"25","button_width_tablet":"50","step_next_label":"Next","step_previous_label":"Previous","step_type":"number_text","step_icon_shape":"circle"}'
                                                                 data-widget_type="form.default">
                                                                <div class="elementor-widget-container">
                                                                    @using (Html.BeginForm("BusinessDetails", "Vendor", FormMethod.Post))
                                                                    {
                                                                        @*<input type="hidden" name="post_id" value="88" />
                                                                        <input type="hidden" name="form_id" value="3477432" />
                                                                        <input type="hidden" name="queried_id" value="88" />*@
                                                                        <div class="elementor-form-fields-wrapper elementor-labels-above">
                                                                            <div class="elementor-field-type-text elementor-field-group elementor-column elementor-field-group-name elementor-col-100">
                                                                                Brand Name *
                                                                                @Html.TextBoxFor(a => a.CompanyName, new { placeholder = "Company Name", @required = "required", @class = "elementor-field elementor-size-sm elementor-field-textual" })
                                                                                <span class="text-danger">
                                                                                    @Html.ValidationMessageFor(a => a.CompanyName)
                                                                                </span>
                                                                            </div>
                                                                            <div class="elementor-field-type-text elementor-field-group elementor-column elementor-field-group-name elementor-col-100">
                                                                                Legal Company Name *
                                                                                @Html.TextBoxFor(a => a.LegalCompanyName, new { placeholder = "Legal Company Name", @required = "required", @class = "elementor-field elementor-size-sm elementor-field-textual" })
                                                                                <span class="text-danger">
                                                                                    @Html.ValidationMessageFor(a => a.LegalCompanyName)
                                                                                </span>
                                                                            </div>
                                                                            <div class="elementor-field-type-email elementor-field-group elementor-column elementor-field-group-email elementor-col-100 elementor-field-required">
                                                                                Business Filing Status *
                                                                                @Html.DropDownListFor(a => a.BusinessFilingStatus, Model.BusinessStatuList, "Select Business Filing Status ", new { @required = "required", @class = "elementor-field elementor-size-sm elementor-field-textual" })
                                                                                <span class="text-danger">
                                                                                    @Html.ValidationMessageFor(a => a.BusinessFilingStatus)
                                                                                </span>
                                                                            </div>
                                                                            <div class="elementor-field-type-text elementor-field-group elementor-column elementor-field-group-field_a6f70fc elementor-col-100">
                                                                                PAN Details *
                                                                                @Html.TextBoxFor(a => a.PAN_No, new { placeholder = "Pan Card Number", @required = "required", @class = "elementor-field elementor-size-sm elementor-field-textual" })
                                                                                <span class="text-danger">
                                                                                    @Html.ValidationMessageFor(a => a.PAN_No)
                                                                                </span>
                                                                            </div>
                                                                            <div class="elementor-field-type-text elementor-field-group elementor-column elementor-field-group-name elementor-col-100">
                                                                                Registered Address *
                                                                                @Html.TextBoxFor(a => a.RegisteredAddress, new { placeholder = "Registered Address", @required = "required", @class = "elementor-field elementor-size-sm elementor-field-textual" })
                                                                                <span class="text-danger">
                                                                                    @Html.ValidationMessageFor(a => a.RegisteredAddress)
                                                                                </span>
                                                                            </div>
                                                                            <div class="elementor-field-type-textarea elementor-field-group elementor-column elementor-field-group-message elementor-col-100">
                                                                                Operating Address *
                                                                                @Html.TextBoxFor(a => a.OperatingAddress, new { placeholder = "Operating Address", @required = "required", @class = "elementor-field elementor-size-sm elementor-field-textual" })
                                                                                <span class="text-danger">
                                                                                    @Html.ValidationMessageFor(a => a.OperatingAddress)
                                                                                </span>
                                                                            </div>
                                                                            <div class="elementor-field-group elementor-column elementor-field-type-submit elementor-col-25 e-form__buttons elementor-md-50">
                                                                                <button type="submit" name="prevBtn" class="elementor-button elementor-size-sm" value="Previous">
                                                                                    <span> <span class="elementor-button-icon"> </span> <span class="elementor-button-text">Previous</span> </span>
                                                                                </button>
                                                                            </div>
                                                                            <div class="elementor-field-group elementor-column elementor-field-type-submit elementor-col-25 e-form__buttons elementor-md-50">
                                                                                <button type="submit" name="nextBtn" class="elementor-button elementor-size-sm" value="Next">
                                                                                    <span> <span class="elementor-button-icon"> </span> <span class="elementor-button-text">Next</span> </span>
                                                                                </button>
                                                                            </div>
                                                                        </div>
                                                                    }
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </article>
            </main>
        </div>
    </div>
</div>



