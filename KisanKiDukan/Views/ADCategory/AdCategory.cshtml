@model IEnumerable<KisanKiDukan.Models.Domain.ADCategory>
@{
    ViewBag.Title = "AdCategory";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@*<h2>AdCategory</h2>*@
<div style="width:60%">
    <h4>Categories For AD</h4>
    <a href="/ADCategory/Add" class="btn btn-primary">Add more</a>
    <table class="table table-striped">
        <tr>
            <th>CategoryName</th>
            <th>Action</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CategoryName</td>
                <td>
                    <a href="/ADCategory/Edit?id=@item.Id" class="btn btn-info">Edit</a>
                    <a href="/ADCategory/Delete?id=@item.Id" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </table>
</div>




